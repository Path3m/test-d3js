    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch firefox",
            "request": "launch",
            "type": "firefox",
            "reAttach": true,
            "file": "${workspaceFolder}/steamgraph_example.html"
        },
        {
            "name": "Launch chrome",
            "request": "launch",
            "type": "chrome",
            "file": "${workspaceFolder}/steamgraph_example.html"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "launch function test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/package-func-test/function_test.js"
        }
    ]
}